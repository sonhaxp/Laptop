            <script>
                function formatCash(str) {
                    return str.split('').reverse().reduce((prev, next, index) => {
                        return ((index % 3) ? next : (next + ',')) + prev
                    })
                }
                GetDS = function () {
                    $.ajax({
                        url: "/api/Statistics",
                        method: "GET",
                        contentType: "json",
                        crossDomain: true,
                        dataType: 'json',
                        success: function (res) {
                            $("#laptop-1-ngay").text(res[0] + " chiếc");
                            $("#laptop-1-thang").text(res[1] + " chiếc");
                            $("#doanh-thu-ngay").text(formatCash(""+res[2]) + "vnđ");
                            $("#doanh-thu-thang").text(formatCash(""+res[3]) + "vnđ");
                        },
                        error: function () {
                            console.log("Load api get thất bại");
                        }
                    });
                }
                GetDS();
                GetReveneManufacturer = function () {
                    $.ajax({
                        url: "/getReveneManufacturer",
                        method: "GET",
                        contentType: "json",
                        crossDomain: true,
                        dataType: 'json',
                        success: function (res) {
                            var len = res.length;
                            var hang = []
                            var cot = []
                            for (var i = 0; i < len; i++) {
                                hang.push(res[i][0])
                                cot.push(Number(res[i][1]))
                            }
                            var ctx1 = $("#worldwide-sales").get(0).getContext("2d");
                            var myChart1 = new Chart(ctx1, {
                                type: "bar",
                                data: {
                                    labels: hang,
                                    datasets: [{
                                        label: "Doanh thu hãng sản xuất",
                                        data: cot,
                                        backgroundColor: "rgba(0, 156, 255, .7)"
                                    }
                                    ]
                                },
                                options: {
                                    responsive: true
                                }
                            });
                        },
                        error: function () {
                            console.log("Load api get thất bại");
                        }
                    });
                }
                GetReveneManufacturer()
                GetRevene = function () {
                    $.ajax({
