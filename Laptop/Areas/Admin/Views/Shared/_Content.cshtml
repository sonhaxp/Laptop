                            console.log("Load api get thất bại");
                        }
                    });
                }
                GetRevene();
                getTopUser = function () {
                    $.ajax({
                        url: "/getTopUser",
                        method: "GET",
                        contentType: "json",
                        crossDomain: true,
                        dataType: 'json',
                        success: function (res) {
                            var len = res.length;
                            var table = ''
                            for (var i = 0; i < len; i++) {
                                table = table + '<tr>';
                                table = table + '<td>' + res[i][0] + '</td>';
                                table = table + '<td>' + res[i][1] + '</td>';
                                table = table + '<td>' + res[i][2] + '</td>';
                                table = table + '<td>' + res[i][3] + '</td>';
                                table = table + '<td>' + formatCash("" + res[i][4]) + "vnđ" + '</td>';
